{
  "name": "transportation-service-plan",
  "version": "1.0.0",
  "description": "Portland Bureau of Transportation Service Plan 2035",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all --parallel html css serve watch",
    "serve": "live-server build/ --entry-file=200.html",
    "html": "cpy src/*.html build/",
    "precss": "mkdirp build/css",
    "css": "myth src/css/style.css build/css/style.css -c -s",
    "watch": "rerun-script",
    "test": "test"
  },
  "watches": {
    "html": "src/**/*.html",
    "css": "src/**/*.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaswise/transportation-service-plan.git"
  },
  "author": "Nikolas WIse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nikolaswise/transportation-service-plan/issues"
  },
  "homepage": "https://github.com/nikolaswise/transportation-service-plan#readme",
  "devDependencies": {
    "cpy-cli": "^1.0.1",
    "live-server": "^1.1.0",
    "mkdirp": "^0.5.1",
    "myth": "github:pin3da/myth",
    "npm-run-all": "^3.1.1",
    "rerun-script": "^0.6.0"
  }
}
